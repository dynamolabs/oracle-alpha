    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "uuid": "^13.0.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
