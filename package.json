{
  "name": "oracle-alpha",
  "version": "0.1.0",
  "description": "On-chain Reliable Alpha Compilation & Learning Engine",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node --transpile-only src/api/server.ts",
    "dev": "ts-node --transpile-only src/api/server.ts",
